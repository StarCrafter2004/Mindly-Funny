{
    "kind": "collectionType",
    "collectionName": "t_users",
    "info": {
        "singularName": "t-user",
        "pluralName": "t-users",
        "displayName": "TUser"
    },
    "options": {
        "draftAndPublish": false
    },
    "pluginOptions": {},
    "attributes": {
        "telegram_id": {
            "type": "biginteger",
            "required": true,
            "unique": true
        },
        "username": {
            "type": "string"
        },
        "firstName": {
            "type": "string"
        },
        "lastName": {
            "type": "string"
        },
        "sex": {
            "type": "string",
            "default": "notSet"
        },
        "age": {
            "type": "string"
        },
        "profession": {
            "type": "string"
        },
        "country": {
            "type": "string"
        },
        "isProfileComplete": {
            "type": "boolean",
            "default": false
        },
        "isPremium": {
            "type": "boolean"
        },
        "premiumUntil": {
            "type": "datetime"
        },
        "freePremiumCounter": {
            "type": "integer",
            "default": 0
        },
        "freeLivesCounter": {
            "type": "integer",
            "default": 0
        },
        "FreePremActivated": {
            "type": "boolean",
            "default": false
        },
        "isTestPassed": {
            "type": "boolean",
            "default": false
        },
        "lives": {
            "type": "integer",
            "required": true,
            "default": 3
        },
        "lastDailyCheck": {
            "type": "datetime"
        }
    }
}
